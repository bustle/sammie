{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Transform": "AWS::Serverless-2016-10-31",
  "Description": "Httptest2",
  "Parameters": {
    "stackName": {
      "Type": "String",
      "Default": "httptest2"
    },
    "bucketName": {
      "Type": "String",
      "Default": "sam-uploads-123532619935"
    },
    "environment": {
      "Type": "String",
      "Default": "development"
    }
  },
  "Resources": {
    "Httptest2Function": {
      "Type": "AWS::Serverless::Function",
      "Properties": {
        "FunctionName": {
          "Ref": "AWS::StackName"
        },
        "CodeUri": "index.js",
        "Handler": "index.handler",
        "Runtime": "nodejs12.x",
        "MemorySize": 256,
        "Timeout": 10,
        "Events": {
          "HttpApiEvent": {
            "Type": "HttpApi"
          }
        }
      }
    }
  },
  "Outputs": {
    "region": {
      "Value": {
        "Ref": "AWS::Region"
      }
    },
    "apiId": {
      "Value": {
        "Ref": "ServerlessHttpApi"
      }
    }
  }
}
